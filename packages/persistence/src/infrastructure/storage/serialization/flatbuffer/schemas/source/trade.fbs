// Trade schema for FlatBuffers
namespace FBTrade;

table Trade {
  id: ulong;           // Trade ID
  p: double;           // Price
  q: double;           // Quantity
  qq: double;          // Quote quantity
  t: ulong;            // Timestamp
  m: bool;             // Is buyer maker
}

table TradeBatch {
  trades: [Trade];     // Array of trades
  count: int;          // Number of trades
}

root_type TradeBatch;
